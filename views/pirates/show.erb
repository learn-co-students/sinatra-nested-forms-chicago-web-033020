<h1>Pirate</h1>
  <div class="pirate">
  <h2>Name: <%=@pirate.name%></h2>
  <h2>Weight: <%=@pirate.weight%></h2>
  <h2>Height: <%=@pirate.height%></h2>
  </div>
<h1>Ships</h1>
  <%@ships.each do |ship|%>
  <div class="ships">
  <p>Name: <%=ship.name%></p>
  <p>Type: <%=ship.type%></p>
  <p>Booty: <%=ship.booty%></p>
  </div>
  <%end%>
